<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	<link href="~/Content/weui.min.css" rel="stylesheet" />
	<link href="~/Content/example.css" rel="stylesheet" />
	@*@Styles.Render("~/Content/weuicss")*@
	<script src="~/Scripts/jquery-2.2.3.min.js"></script>
</head>
<body ng-app="@ViewBag.NgScopeName" class="ng-scope">
	<div class="container" id="container">
		@RenderBody()
	</div>
	<script src="~/Scripts/Base.js"></script>
	@RenderSection("scriptsContainer", required: false)
	@*@Scripts.Render("~/bundles/weuijs")*@
	<script src="~/Scripts/angular/angular.min.js"></script>
	<script src="~/Scripts/angular/angular-route.min.js"></script>
	@RenderSection("scripts", required: false)
</body>
</html>
